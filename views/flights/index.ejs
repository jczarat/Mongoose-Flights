<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Flights</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
</head>
<body>
    <thead>
        <tr>
          <th>Airline&nbsp;</th>
          <th>Airport&nbsp;</th>
          <th>Flight Number&nbsp;</th>
          <th>Depature Time&nbsp;</th>
        </tr>
      </thead>
    <% flights.forEach(flight => { %>
        <tr>
            <td><%= flight.airline %></td>
            <td><%= flight.airport %></td>
            <td><%= flight.flightNo %></td>
            <td><%= m.cast.join(', ') %></td>
          </tr>  
    <% }) %>
</body>
</html>